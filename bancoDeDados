('aluno', '58002', 'Rodrigo', 'rodrigo@1', 'senha2');


CREATE TABLE produtos (
    id INT PRIMARY KEY AUTO_INCREMENT,
    nome VARCHAR(255),
    categoria VARCHAR(255),
    descricao VARCHAR(255),
    preco FLOAT,
    imagem VARCHAR(255) -- Coluna para armazenar o caminho da imagem
);
