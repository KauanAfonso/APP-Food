CREATE DATABASE app_food;

CREATE TABLE usuariosetec( id int PRIMARY KEY AUTO_INCREMENT, tipo varchar(255), nsa varchar(255), nome varchar(255), username varchar(255), password varchar(255) );

INSERT INTO usuariosetec (tipo, nsa, nome, username, password) VALUES
